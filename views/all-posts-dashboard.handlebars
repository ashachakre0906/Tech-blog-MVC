<!---All post from all users-->
<div class ='row'>
    <div class='col-auto'>
        {{#if logged_in}}
        <h2>Hello,{{user.username}}!</h2>
        {{/if}}
    </div>
</div>
{{#if posts.length}}
<div class = "card flex-column">
    {{#each posts as |post|}}
    {{> editItem post}}
    {{post}}
    {{/each}}
</div>
{{/if}}